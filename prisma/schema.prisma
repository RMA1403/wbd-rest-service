generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Category {
  TECHNOLOGY
  COMEDY
  HORROR
}

model DummyTable {
  description String @unique

  @@map("dummy_table")
}

model PremiumPodcasts {
  idPodcast    Int      @id
  title        String
  urlThumbnail String
  description  String
  category     Category

  @@map("premium_podcasts")
}
